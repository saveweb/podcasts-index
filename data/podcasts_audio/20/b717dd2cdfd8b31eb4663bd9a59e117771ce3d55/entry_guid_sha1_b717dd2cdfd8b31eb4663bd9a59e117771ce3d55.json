    "itunes_explicit": null,
    "image": {
        "href": "https://assets.fireside.fm/file/fireside-images/podcasts/images/2/271eaae7-5c3a-4a2c-aed6-39e245b9fb53/episodes/0/0466591d-5bfe-409b-839b-ad192022859b/cover.jpg"
