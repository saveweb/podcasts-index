    "image": {
        "href": "https://assets.fireside.fm/file/fireside-images/podcasts/images/2/271eaae7-5c3a-4a2c-aed6-39e245b9fb53/episodes/7/79e371c5-f28d-4ad2-82c0-3d79b702f3b7/cover.jpg"
